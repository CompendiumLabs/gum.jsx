{
  "name": "gum-jsx",
  "version": "1.2.4",
  "description": "Language for vector graphics generation.",
  "type": "module",
  "author": "Douglas Hanley",
  "homepage": "https://github.com/CompendiumLabs/gum.jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CompendiumLabs/gum.jsx"
  },
  "license": "MIT",
  "files": [
    "src",
    "src/fonts",
    "docs",
    "docs/text",
    "docs/code",
    "docs/gallery",
    "prompt",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./src/gum.js"
    },
    "./eval": {
      "types": "./index.d.ts",
      "import": "./src/eval.js"
    },
    "./fonts/*": "./src/fonts/*",
    "./docs/*": "./docs/*",
    "./prompt/*": "./prompt/*",
    "./skill/*": "./skill/*"
  },
  "scripts": {
    "skill": "bun ./scripts/skill.js"
  },
  "bin": {
    "gum": "./scripts/cli.js",
    "gum-server": "./scripts/server.js"
  },
  "dependencies": {
    "acorn-jsx": "^5.3.2",
    "emojibase-regex": "^17.0.0",
    "linebreak": "^1.1.0",
    "opentype.js": "^1.3.4",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0"
  },
  "optionalDependencies": {
    "@mathjax/src": "^4.1.0",
    "commander": "^14.0.2",
    "express": "^5.2.1",
    "mathjax": "^4.1.0"
  }
}
