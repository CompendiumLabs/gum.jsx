{
  "name": "gum-jsx",
  "version": "1.3.1",
  "description": "Language for vector graphics generation.",
  "type": "module",
  "author": "Douglas Hanley",
  "homepage": "https://github.com/CompendiumLabs/gum.jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CompendiumLabs/gum.jsx"
  },
  "license": "MIT",
  "files": [
    "src",
    "src/fonts",
    "docs",
    "docs/text",
    "docs/code",
    "docs/gallery",
    "prompt",
    "skill",
    "skill/references",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./src/gum.js"
    },
    "./eval": {
      "types": "./index.d.ts",
      "import": "./src/eval.js"
    },
    "./render": {
      "types": "./index.d.ts",
      "import": "./src/render.js"
    },
    "./meta": {
      "types": "./index.d.ts",
      "import": "./src/meta.js"
    },
    "./fonts/*": "./src/fonts/*",
    "./docs/*": "./docs/*",
    "./prompt/*": "./prompt/*",
    "./skill/*": "./skill/*"
  },
  "scripts": {
    "cli": "bun ./scripts/cli.js",
    "test": "bun ./scripts/test.js",
    "skill": "bun ./scripts/skill.js",
    "claude": "bun ./scripts/skill.js && bun ./scripts/claude.ts"
  },
  "bin": {
    "gum-cli": "./scripts/cli.js",
    "gum-mcp": "./scripts/mcp.js"
  },
  "dependencies": {
    "acorn-jsx": "^5.3.2",
    "emojibase-regex": "^17.0.0",
    "linebreak": "^1.1.0",
    "opentype.js": "^1.3.4"
  },
  "optionalDependencies": {
    "@mathjax/src": "^4.1.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@resvg/resvg-js": "^2.6.2",
    "commander": "^14.0.3",
    "express": "^5.2.1",
    "mathjax": "^4.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.72.1"
  }
}
